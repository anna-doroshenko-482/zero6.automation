{
  "name": "briz-billing",
  "version": "1.0.1",
  "description": "Webdriver TS mocha framework",
  "main": "index.js",
  "scripts": {
    "prettier:docker": "prettier --write '**/**/*.ts'",
    "eslint": "npx eslint --fix --ext .ts,.js helpers lib page_objects specs testData utils",
    "selenoid:wdio:all": "sleep 1 && wdio config/runners/selenoid.conf.ts --spec",
    "selenoid:wdio:suite": "sleep 1 && wdio config/runners/selenoid.conf.ts --suite testSpecs",
    "selenoid:path:specific": "sleep 1 && wdio config/runners/selenoid.conf.ts --files --spec ",
    "selenoid:name:specific": "sleep 1 && wdio config/runners/selenoid.conf.ts --mochaOpts.grep ",
    "selenoid:run": "sleep 1 && wdio config/runners/selenoid.conf.ts --files --spec ./specs/web/start.test.ts",
    "test:local:chrome:specific": "wdio config/runners/chrome.conf.ts --files --spec specs/web/zerosix/menu.test.ts",
    "test:local:chrome:testSuite": "wdio config/runners/chrome.conf.ts --suite testSpecs",
    "test:local:chrome": "wdio config/runners/chrome.conf.ts --files --spec ./specs/web/start.test.ts",
    "test:local:chrome:tags": "wdio config/runners/chrome.conf.ts --mochaOpts.grep smoke",
    "test:local:firefox": "wdio config/runners/firefox.conf.ts --files --spec ./specs/web/billing/create.new.subscriber.test.ts",
    "allure:open": "allure generate allure-results --clean -o allure-report && allure open",
    "artifacts:cleanup": "rm -rf allure-report allure-results allure-results.zip video-result test-results screenshots .tmp selenoid/logs selenoid/video selenoid/screenshots",
    "allure:add:reportUploading": "node scripts/upload-generate-report",
    "selenoid:test": "npm-run-all artifacts:cleanup selenoid:wdio:suite allure:add:reportUploading --continue-on-error",
    "mocha:suite": "mocha --config config/runners/.mocharc.js -g '.*'",
    "mocha:specific": "mocha --config config/runners/.mocharc.js -g "
  },
  "author": "Briz Team",
  "devDependencies": {
    "@testdeck/mocha": "0.1.2",
    "@types/chai": "4.2.22",
    "@types/chai-like": "1.1.1",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/node-fetch": "3.0.2",
    "@types/sinon": "^10.0.3",
    "@types/sinon-chai": "3.2.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@wdio/allure-reporter": "^7.20.7",
    "@wdio/cli": "^7.20.9",
    "@wdio/devtools-service": "^7.20.9",
    "@wdio/firefox-profile-service": "^7.20.7",
    "@wdio/junit-reporter": "^7.20.7",
    "@wdio/local-runner": "^7.20.9",
    "@wdio/mocha-framework": "^7.20.7",
    "@wdio/selenium-standalone-service": "^7.20.8",
    "@wdio/shared-store-service": "^7.20.9",
    "allure-commandline": "^2.13.8",
    "allure-mocha": "^2.0.0-beta.13",
    "babel-preset-env": "^1.7.0",
    "chai": "4.3.4",
    "chai-like": "1.1.1",
    "chalk": "4.1.1",
    "cpx": "^1.5.0",
    "devtools": "^7.7.4",
    "dotenv": "10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "0.7.1",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "5.5.3",
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "log4js": "6.3.0",
    "md5-typescript": "^1.0.5",
    "memory-cache": "^0.2.0",
    "mocha": "^9.0.2",
    "mocha-junit-reporter": "2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "node-fetch": "2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "process-rerun": "0.2.4",
    "sinon": "11.1.1",
    "sinon-chai": "3.7.0",
    "source-map-support": "^0.5.19",
    "textract": "^2.5.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "typescript-eslint-parser": "^22.0.0",
    "uuid": "^8.3.2",
    "wdio-intercept-service": "4.1.7",
    "wdio-video-reporter": "3.1.2",
    "xml2js": "^0.4.23",
    "zip-folder": "^1.0.0"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "cross-var": "^1.1.0"
  }
}
